<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Companylogin</servlet-name>
        <servlet-class>Company.Companylogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Adminlogin</servlet-name>
        <servlet-class>Company.Adminlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Schoollogin</servlet-name>
        <servlet-class>School.Schoollogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Studentlogin</servlet-name>
        <servlet-class>Student.Studentlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Transaction</servlet-name>
        <servlet-class>Aftertransaction.Transaction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Companylogin</servlet-name>
        <url-pattern>/companylogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Adminlogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Schoollogin</servlet-name>
        <url-pattern>/schoollogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Studentlogin</servlet-name>
        <url-pattern>/studentlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Transaction</servlet-name>
        <url-pattern>/transaction</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>default.html</welcome-file>
    </welcome-file-list>
</web-app>
